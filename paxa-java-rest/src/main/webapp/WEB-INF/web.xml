        <init-param>
            <param-name>jersey.config.server.provider.packages</param-name>
            <param-value>se.hexabit.paxa.rest</param-value>
        </init-param>
        <init-param>
            <param-name>jersey.config.server.provider.classnames</param-name>
            <param-value>se.hexabit.paxa.rest.filter.CORSResponseFilter</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
